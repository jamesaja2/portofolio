# Environment Variables Example
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your_publishable_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
SUPABASE_DB_URL=postgresql://postgres:your_password@your_host:6543/postgres

# Neon Database (if using)
DATABASE_URL=postgresql://user:password@host/database?sslmode=require

# TinyMCE Editor API Key
NEXT_PUBLIC_TINYMCE_API_KEY=your_tinymce_api_key

# Admin Seeding (for scripts/seed_admin.js)
SEED_ADMIN_EMAIL=your_admin_email@example.com
SEED_ADMIN_PASSWORD=your_secure_password
